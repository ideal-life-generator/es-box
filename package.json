{
  "name": "startmusic",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "scripts": {
    "lint": "eslint ./",
    "dev:graphql": "better-npm-run dev:graphql",
    "dev:interface": "better-npm-run dev:interface",
    "dev": "parallelshell \"rimraf prod\" \"better-npm-run dev:graphql\" \"better-npm-run dev:interface\"",
    "prod:graphql": "better-npm-run prod:graphql",
    "prod:interface": "better-npm-run prod:interface",
    "prod": "parallelshell \"rimraf prod\" \"better-npm-run prod:graphql\" \"better-npm-run prod:interface\"",
    "start": "npm run dev",
    "pretify": "prettier --write ./interface/**/*.js"
  },
  "betterScripts": {
    "dev:graphql": {
      "command": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config=./webpack/graphql.js --watch",
      "env": {
        "SERVER": true
      }
    },
    "dev:interface": {
      "command": "node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config=./webpack/interface.js"
    },
    "prod:graphql": {
      "command": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config=./webpack/graphql.js",
      "env": {
        "PRODUCTION": true,
        "SERVER": true
      }
    },
    "prod:interface": {
      "command": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config=./webpack/interface.js",
      "env": {
        "PRODUCTION": true
      }
    }
  },
