{
  "name": "observable-sandbox",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "scripts": {
    "lint": "eslint ./",
    "dev:server": "better-npm-run dev:server",
    "dev:client": "better-npm-run dev:client",
    "dev": "concurrently \"better-npm-run dev:server\" \"better-npm-run dev:client\""
  },
  "betterScripts": {
    "dev:server": {
      "command": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config=./webpack/server.js --watch",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev:client": {
      "command": "node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config=./webpack/client.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "concurrently": "^3.5.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "html-webpack-plugin": "^2.30.1",
    "reload-server-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.3.0",
    "express": "^4.16.2",
    "koa": "^2.3.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-jss": "^7.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-observable": "^0.17.0",
    "redux-thunk": "^2.2.0"
  }
}
