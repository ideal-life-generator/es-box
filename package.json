{
  "name": "vue-box",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "scripts": {
    "lint": "eslint ./",
    "dev:graphql": "better-npm-run dev:graphql",
    "dev:interface": "better-npm-run dev:interface",
    "dev": "parallelshell \"rimraf prod\" \"better-npm-run dev:graphql\" \"better-npm-run dev:interface\"",
    "prod:graphql": "better-npm-run prod:graphql",
    "prod:interface": "better-npm-run prod:interface",
    "prod": "parallelshell \"rimraf prod\" \"better-npm-run prod:graphql\" \"better-npm-run prod:interface\"",
    "start": "npm run dev"
  },
  "betterScripts": {
    "dev:graphql": {
      "command": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config=./webpack/graphql.js --watch",
      "env": {
        "SERVER": true
      }
    },
    "dev:interface": {
      "command": "node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config=./webpack/interface.js"
    },
    "prod:graphql": {
      "command": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config=./webpack/graphql.js",
      "env": {
        "PRODUCTION": true,
        "SERVER": true
      }
    },
    "prod:interface": {
      "command": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config=./webpack/interface.js",
      "env": {
        "PRODUCTION": true
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "copy-webpack-plugin": "^4.4.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "parallelshell": "^3.0.2",
    "reload-server-webpack-plugin": "^1.0.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "start-server-webpack-plugin": "^2.2.1",
    "style-loader": "^0.20.1",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "apollo-server-koa": "^1.3.2",
    "chalk": "^2.3.1",
    "deep-assign": "^2.0.0",
    "fast-clone": "^1.4.2",
    "fast-deep-equal": "^1.0.0",
    "graphql": "^0.13.0",
    "graphql-tools": "^2.21.0",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-cors": "^0.0.16",
    "koa-proxy": "^0.9.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "node-fetch": "^2.0.0",
    "qs": "^6.5.1",
    "source-map-support": "^0.5.3",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "youtube-dl": "^1.12.2",
    "ytdl-core": "^0.20.1",
    "ytdl-getinfo": "^1.0.3"
  }
}
