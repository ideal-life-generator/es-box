{
  "name": "vue-box",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "scripts": {
    "lint": "eslint ./",
    "dev:app": "better-npm-run dev:app",
    "dev:client": "better-npm-run dev:client",
    "dev": "parallelshell \"rimraf build\" \"better-npm-run dev:app\" \"better-npm-run dev:client\"",
    "build:app": "better-npm-run build:app",
    "build:client": "better-npm-run build:client",
    "build": "parallelshell \"rimraf build\" \"better-npm-run build:app\" \"better-npm-run build:client\"",
    "start": "npm run dev"
  },
  "betterScripts": {
    "dev:app": {
      "command": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config=./webpack/app.js --watch",
      "env": {
        "SERVER": true
      }
    },
    "dev:client": {
      "command": "node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config=./webpack/client.js"
    },
    "build:app": {
      "command": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config=./webpack/app.js",
      "env": {
        "PRODUCTION": true,
        "SERVER": true
      }
    },
    "build:client": {
      "command": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config=./webpack/client.js",
      "env": {
        "PRODUCTION": true
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-vue": "^3.13.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jade": "^1.11.0",
    "node-sass": "^4.7.2",
    "parallelshell": "^3.0.2",
    "reload-server-webpack-plugin": "^1.0.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "vue-loader": "^13.5.0",
    "vue-template-compiler": "^2.5.8",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.3",
    "apollo-client": "^2.0.3",
    "apollo-link": "^1.0.3",
    "apollo-link-http": "^1.2.0",
    "apollo-server-koa": "^1.2.0",
    "axios": "^0.17.1",
    "chalk": "^2.3.0",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "graphql-tools": "^2.9.0",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-cors": "^0.0.16",
    "koa-proxy": "^0.9.0",
    "koa-router": "^7.3.0",
    "koa-static": "^4.0.2",
    "vue": "^2.5.8",
    "vue-apollo": "^3.0.0-alpha.1",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.8",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  }
}
