{
  "name": "startmusic",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "scripts": {
    "lint": "eslint ./",
    "dev:graphql": "better-npm-run dev:graphql",
    "dev:interface": "better-npm-run dev:interface",
    "dev":
      "parallelshell \"rimraf prod\" \"better-npm-run dev:graphql\" \"better-npm-run dev:interface\"",
    "prod:graphql": "better-npm-run prod:graphql",
    "prod:interface": "better-npm-run prod:interface",
    "prod":
      "parallelshell \"rimraf prod\" \"better-npm-run prod:graphql\" \"better-npm-run prod:interface\"",
    "start": "npm run dev",
    "pretify": "prettier --write ./interface/**/*.js",
    "precommit": "pretty-quick --staged"
  },
  "betterScripts": {
    "dev:graphql": {
      "command":
        "node -r babel-register ./node_modules/webpack/bin/webpack.js --config=./webpack/graphql.js --watch",
      "env": {
        "SERVER": true
      }
    },
    "dev:interface": {
      "command":
        "node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config=./webpack/interface.js"
    },
    "prod:graphql": {
      "command":
        "node -r babel-register ./node_modules/webpack/bin/webpack.js --config=./webpack/graphql.js",
      "env": {
        "PRODUCTION": true,
        "SERVER": true
      }
    },
    "prod:interface": {
      "command":
        "node -r babel-register ./node_modules/webpack/bin/webpack.js --config=./webpack/interface.js",
      "env": {
        "PRODUCTION": true
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-vue": "^4.4.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "husky": "^0.14.3",
    "jade": "^1.11.0",
    "node-sass": "^4.8.3",
    "parallelshell": "^3.0.2",
    "prettier": "1.11.1",
    "pretty-quick": "^1.4.1",
    "reload-server-webpack-plugin": "^1.0.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "source-map-support": "^0.5.4",
    "start-server-webpack-plugin": "^2.2.5",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
