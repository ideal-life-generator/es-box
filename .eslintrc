{
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
     "parser": "babel-eslint",
  },
  plugins: ["import"],
  "extends": [
    "airbnb-base",
    "plugin:vue/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "semi": [1, "never"],
    "comma-dangle": [1, "always-multiline"],
    "linebreak-style": 0,
    "no-debugger": 1,
    "spaced-comment": 0,
    "max-len": [1, 150],
    "no-console": 0,
    "import/prefer-default-export": 0,
    "arrow-body-style": 1,
    "no-bitwise": 1,
    "consistent-return": 0,
    "no-return-await": 0,
    "class-methods-use-this": 1,
    "array-callback-return": 1,
    "no-param-reassign": 0,
    "no-return-assign": 0,
    "no-throw-literal": 0,
    "no-underscore-dangle": 1,
    "function-paren-newline": 1,
    "arrow-parens": 0,
    "object-curly-newline": 0,
    "object-property-newline": 0,
    "default-case": 0,
  },
}