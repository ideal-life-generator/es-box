{
  "presets": [
    "env",
    "stage-0"
  ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    "dynamic-import-node",
    "transform-decorators-legacy",
    ["module-resolver", {
      "alias": {
        "__": "./core",
        "_": "./interface/core",
      },
    }],
  ]
}
